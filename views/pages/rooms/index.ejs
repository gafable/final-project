<div class="wrap">
    <div class="container">
        <div class="row">
            <div id="availability">
                <form action="/rooms/check-availabilty" method="GET" id="availabilityForm">

                    <div class="a-col alternate col-md-5">
                        <div class="input-field">
                            <label for="date-start">Check In</label>
                            <input type="text" class="form-control" id="date-start" name="checkIn" />
                        </div>
                    </div>

                    <div class="a-col alternate col-md-5">
                        <div class="input-field">
                            <label for="date-end">Check Out</label>
                            <input type="text" class="form-control" id="date-end" name="checkOut" />
                        </div>
                    </div>

                    <div class="a-col action col-md-2">
                        <a onclick="document.getElementById('availabilityForm').submit()">
                            <span>Check</span> Availability
                        </a>
                    </div>
                </form>
            </div>
        </div>
    </div>
</div>


<div id="fh5co-counter-section" class="fh5co-counters">
