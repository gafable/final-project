<!-- User Accessibility-->
<div class="wrap">
    <div class="container">
        <div class="row">
            <div id="availability">
                <form action="/rooms/" method="GET" id="availabilityForm">

                    <div class="a-col alternate col-md-5">
                        <div class="input-field">
                            <label for="date-start">Check In</label>
                            <input type="text" class="form-control" id="date-start" name="date-start" />
                        </div>
                    </div>

                    <div class="a-col alternate col-md-5">
                        <div class="input-field">
                            <label for="date-end">Check Out</label>
                            <input type="text" class="form-control" id="date-end" name="date-end" />
                        </div>
                    </div>

                    <div class="a-col action col-md-2">
                        <a href="#" onclick="document.getElementById('availabilityForm').submit()">
                            <span>Check</span> Availability
                        </a>
                    </div>
                </form>
            </div>
        </div>
    </div>
</div>
<!-- End of User Acessibility -->

<!-- Display Rating -->
<div id="fh5co-counter-section" class="fh5co-counters">
    <div class="container">
        <div class="row">
            <div class="col-md-3 text-center">
                <span class="fh5co-counter js-counter" data-from="0" data-to="100" data-speed="5000" data-refresh-interval="50"></span>
                <span class="fh5co-counter-label">User Access</span>
            </div>
            <div class="col-md-3 text-center">
                <span class="fh5co-counter js-counter" data-from="0" data-to="15501" data-speed="5000" data-refresh-interval="50"></span>
                <span class="fh5co-counter-label">Hotels</span>
            </div>
            <div class="col-md-3 text-center">
                <span class="fh5co-counter js-counter" data-from="0" data-to="8200" data-speed="5000" data-refresh-interval="50"></span>
                <span class="fh5co-counter-label">Transactions</span>
            </div>
            <div class="col-md-3 text-center">
                <span class="fh5co-counter js-counter" data-from="0" data-to="8763" data-speed="5000" data-refresh-interval="50"></span>
                <span class="fh5co-counter-label">Rating &amp; Review</span>
            </div>
        </div>
    </div>
</div>

<!-- FeATURED HighQua -->
<div class="row">
    <div class="col-md-12">
        <div class="section-title text-center">
            <h2 class="display-4 text-center mb-4">Featured HighQua Suites </h2>
        </div>
    </div>
</div>




<div class="card">
    <div class="card-body">
        <div class="row">
            <% classTypes.forEach((classType)=> {%>
                <div class="col-md-4">
                    <div id="fh5co-hotel-section">
                        <div class="hotel-content">

                            <div class="hotel-grid">
                                <img src="<%= classType.imageUrl %>" width="100%">
                                <div class="price"><small>For as low as</small><span>&#x20b1; <%= classType.price %>/night</span></div>
                            </div>
                            <div class="desc">
                                <h3>
                                    <a href="#">
                                        <%= classType.name %> &bull;
                                </h3>
                                <button class="btn btn-sm w-50 btn-info more-info"> <i class="fa fa-info mr-3"></i>
                                More Info</button>
                                <input type="hidden" class="room-id" value="<%= classType._id %>">
                                <a href="/bookings/classtypes/<%= classType._id %>" class="btn btn-sm w-50 btn-primary book-now"> <i class="ti-calendar mr-3"></i> Book Now</a>
                            </div>
                        </div>
                    </div>
                </div>
                <%})%>
        </div>
    </div>
</div>



<!-- Modal OverwaterBungalow Suite  -->
<div class="modal fade " id="classTypeModal" tabindex="-1" role="dialog" aria-labelledby="classTypeModal" aria-hidden="true">
    <div class="modal-dialog modal-lg">
        <div class="modal-content">
            <div class="modal-header">
                <button type="button" class="close" data-dismiss="modal" aria-label="Close"> <span
                        aria-hidden="true">&times;</span></button>
                <h2 class="modal-title title" style="color: orange;"></h2>
            </div>
            <div class="modal-body bg-danger" style="padding: 0px;">
                <div class="feature-full-1col m-0">
                    <div class="image">
                        <div class="descrip text-center">
                            <p class="price"><small>For as low as</small><span></span></p>
                        </div>
                    </div>
                    <div class="desc">
                        <ul class="list-group">
                            <h2 style="color: orange;" class="subtitle"> </h2>
                            <p> Room Details</p>
                            <div id="descriptionData"> </div>
                            <p> Room Features </p>
                            <p id="features"></p>
                        </ul>
                        <p><a href="#" class="btn btn-primary btn-luxe-primary">Book Now <i
                                    class="ti-angle-right"></i></a></p>
                    </div>
                </div>
            </div>
        </div>

    </div>
</div>